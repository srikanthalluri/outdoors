{
    "name": "outdoors",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.5.0",
        "@testing-library/user-event": "^7.2.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-scripts": "3.4.1"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "compile:sass": "node-sass src/sass/main.scss src/index.compiled.css",
        "prefix:css": "postcss --use autoprefixer -b \"last 5 versions\" src/index.compiled.css -o src/index.css",
        "build:css": "npm-run-all compile:sass prefix:css"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "autoprefixer": "^9.8.4",
        "node-sass": "^4.14.1",
        "npm-run-all": "^4.1.5",
        "postcss-cli": "^7.1.1"
    }
}